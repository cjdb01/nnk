


template <typename T, size_t I, size_t X, size_t Y, size_t Area>
std::ostream operator<<(std::ostream& os, kohonen<T, I, X, Y, A> k)
{
    for (size_t i = 0; i < Area; ++i)
    {
        os << '(' << i/X << ',' << i/Y << ')' << '\t';
        for (auto cit = k.m_output[i].cbegin(); cit != k.m_output.cend(); ++cit)
        {
            os << *cit;
        }
        
        os << std::endl;
    }
    
    return os;
}